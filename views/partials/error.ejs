<% if (locals.errors) {%>
    <ul style="list-style-type: none; padding: 0;">
        <% errors.forEach(function(error) { %>
          <li style="color: red; background-color: #f8d7da; border: 1px solid #f5c6cb; padding: 10px; margin-bottom: 5px; border-radius: 5px;">
            <%= error.msg %>
          </li>
        <% }) %>
    </ul>
<% } %>
  